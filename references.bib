@article{carter2019,
  title = {Safety and Immunogenicity of Investigational Seasonal Influenza Hemagglutinin {{DNA}} Vaccine Followed by Trivalent Inactivated Vaccine Administered Intradermally or Intramuscularly in Healthy Adults: {{An}} Open-Label Randomized Phase 1 Clinical Trial},
