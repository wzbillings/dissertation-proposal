  keywords = {A(H3N2),Adolescent,Adult,Age Factors,Aged,birth cohorts,Europe,Female,Hemagglutinin Glycoproteins; Influenza Virus,Humans,Immunologic Memory,imprinting,influenza,Influenza A Virus; H3N2 Subtype,Influenza Vaccines,Influenza; Human,Male,Middle Aged,multicentre study,Population Surveillance,Respiratory Tract Infections,Seasons,Sentinel Surveillance,Treatment Outcome,Vaccination,vaccine effectiveness,Vaccine Potency},
  file = {C\:\\Users\\Zane\\Zotero\\storage\\T4WQF75P\\Kissling et al. - 2019 - Low 201819 vaccine effectiveness against influenz.pdf}
}

