@article{fonville2014a,
  title = {Antibody Landscapes after Influenza Virus Infection or Vaccination},
  author = {Fonville, J. M. and Wilks, S. H. and James, S. L. and Fox, A. and Ventresca, M. and Aban, M. and Xue, L. and Jones, T. C. and Le, N. M. H. and Pham, Q. T. and Tran, N. D. and Wong, Y. and Mosterin, A. and Katzelnick, L. C. and Labonte, D. and Le, T. T. and {van der Net}, G. and Skepner, E. and Russell, C. A. and Kaplan, T. D. and Rimmelzwaan, G. F. and Masurel, N. and {de Jong}, J. C. and Palache, A. and Beyer, W. E. P. and Le, Q. M. and Nguyen, T. H. and Wertheim, H. F. L. and Hurt, A. C. and Osterhaus, A. D. M. E. and Barr, I. G. and Fouchier, R. a. M. and Horby, P. W. and Smith, D. J.},
