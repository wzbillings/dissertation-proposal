  shorttitle = {Influenza {{A}}({{H7N9}}) {{Pandemic Preparedness}}},
  author = {Levine, Min Z. and Holiday, Crystal and Bai, Yaohui and Zhong, Weimin and Liu, Feng and Jefferson, Stacie and Gross, F. Liaini and Tzeng, Wen-pin and Fries, Louis and Smith, Gale and Boutet, Philippe and Friel, Damien and Innis, Bruce L. and Mallett, Corey P. and Davis, C. Todd and Wentworth, David E. and York, Ian A. and Stevens, James and Katz, Jacqueline M. and Tumpey, Terrence},
  year = {2022},
