}

@article{henry2018,
  title = {From {{Original Antigenic Sin}} to the {{Universal Influenza Virus Vaccine}}},
  author = {Henry, Carole and Palm, Anna-Karin E. and Krammer, Florian and Wilson, Patrick C.},
  year = {2018},
  month = jan,
