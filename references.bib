  author = {Krammer, Florian and Smith, Gavin J. D. and Fouchier, Ron A. M. and Peiris, Malik and Kedzierska, Katherine and Doherty, Peter C. and Palese, Peter and Shaw, Megan L. and Treanor, John and Webster, Robert G. and {Garc{\'i}a-Sastre}, Adolfo},
  year = {2018},
