@article{linderman2014,
  title = {Potential Antigenic Explanation for Atypical {{H1N1}} Infections among Middle-Aged Adults during the 2013-2014 Influenza Season},
  author = {Linderman, Susanne L. and Chambers, Benjamin S. and Zost, Seth J. and Parkhouse, Kaela and Li, Yang and Herrmann, Christin and Ellebedy, Ali H. and Carter, Donald M. and Andrews, Sarah F. and Zheng, Nai-Ying and Huang, Min and Huang, Yunping and Strauss, Donna and Shaz, Beth H. and Hodinka, Richard L. and {Reyes-Ter{\'a}n}, Gustavo and Ross, Ted M. and Wilson, Patrick C. and Ahmed, Rafi and Bloom, Jesse D. and Hensley, Scott E.},
  year = {2014},
